{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "comma-dangle": 0,
    "func-names": 0,
    "no-alert": 0,
    "no-console": 0,
    "no-else-return": 0,
    "global-require": 0,
    "react/jsx-filename-extension": 0,
    "import/prefer-default-export": 0
  },
  "plugins": ["react"],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./config/webpack.config.dev.js"
      }
    }
  }
}
